{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["./Examples/example.js", "defaultOptions.js", "PrettyDebugJS.js", "package.json", "README.md"],
        "exclude": ["/node_modules"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "referenceTitle": "Pretty Debug Javascript API",
        "disableSort": true,
        "resources": {
            "Github": "https://github.com/shadlyd15/PrettyDebugJS"
        }
    },
    "opts": {
        "destination": "docs",
        "recurse": false,
        "readme": "README.md",
        "lenient": false,
        "sort": false
    }
}