{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["./Examples/example.js", "PrettyDebugJS.js", "defaultOptions.js", "ansiColors.js", "package.json", "README.md"],
        "exclude": ["/node_modules"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 3,
    "templates": {
        "referenceTitle": "API Documentation",
        "disableSort": true,
        "tabNames": {
            "api": "API",
            "examples": "Examples"
        },
        "resources": {
            "Github": "https://github.com/shadlyd15/PrettyDebugJS"
        },
        "examples": {
            "Generic Example": "/PrettyDebugJS/pretty-debug/2.0.0/Examples_example.js.html"
        }
    },
    "opts": {
        "destination": "docs",
        "recurse": true,
        "readme": "README.md",
        "lenient": false,
        "sort": false,
        "tutorials" : "Tutorials/"
    }
}